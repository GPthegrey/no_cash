<h4 class='text-center my-2'><strong>Finalizados</strong></h4>
<div class='border bg-background my-2' >
  <% @completed_deals.each do |deal| %>
    <p class='text-end'><%= deal.created_at.strftime("%Y-%m-%d") %></p>
    <%= render 'dealstatus', deal: deal %>
    <%= render 'review', deal: deal, review: @review %>
  <% end %>
</div>
